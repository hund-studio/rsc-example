{
    "compilerOptions": {
        "allowImportingTsExtensions": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ESNext"
    },
    "exclude": ["node_modules"]
}
